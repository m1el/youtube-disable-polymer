{
  "manifest_version": 2,
  "name": "Disable Polymer for YT",
  "version": "0.0.1",
  "default_locale": "en",
  "description": "Disable Polymer UI for YouTube",
  "author": "Igor null",
  "icons": {
      "128": "logo_128x128.png"
  },
  "content_scripts": [{
     "matches": ["http://www.youtube.com/*", "https://www.youtube.com/*"],
     "js": ["contentScript.js"]
  }]
}
